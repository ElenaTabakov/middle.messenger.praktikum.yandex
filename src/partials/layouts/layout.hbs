<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
     <meta name="viewport" content="width=device-width, initial-scale=1.0" />
     <link rel="shortcut icon" href="#">
    <title>{{title}}</title>
</head>
<body>
  <div id="navigation"></div>
  <script id="navigation-template" type="text/x-handlebars-template">
    <ul>
        {{#each navigation}}
            <li><a href="{{url}}">{{label}}</a></li>
        {{/each}}
    </ul>
</script>
  <main class="wrapper">
    {{> nav/_nav}}
  {{> @partial-block}}</main>
</body>
{{> layouts/allowScss }}
</html>